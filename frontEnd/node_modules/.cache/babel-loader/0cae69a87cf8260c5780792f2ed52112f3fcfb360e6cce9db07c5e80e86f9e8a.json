{"ast":null,"code":"import view4 from '@/components/view4.vue';\nimport corrInd from '../../../../pyCharm/ybcProject/data/corr_ind.json';\nexport default {\n  name: 'page_3',\n  components: {\n    view4\n  },\n  data() {\n    return {\n      chartValue: {\n        targetInd: '',\n        correlation: [['', ''], ['', ''], ['', ''], ['', ''], ['', '']],\n        idx: [0, -1, -2, -3, -4],\n        correlationName: [['', ''], ['', ''], ['', ''], ['', ''], ['', '']]\n      }\n    };\n  },\n  methods: {\n    // 按钮数据读取\n    getBtnData() {\n      const indName = Object.getOwnPropertyNames(corrInd);\n      this.chartValue.targetInd = indName[0];\n      const keys = Object.keys(corrInd[indName[0]]);\n      for (let i = 0; i < 5; i++) {\n        if (keys[i] === '0') {\n          this.chartValue.correlation[0][0] = corrInd[indName[0]][keys[i]][0];\n          this.chartValue.correlation[0][1] = corrInd[indName[0]][keys[i]][1];\n        } else if (keys[i] === '-1') {\n          this.chartValue.correlation[1][0] = corrInd[indName[0]][keys[i]][0];\n          this.chartValue.correlation[1][1] = corrInd[indName[0]][keys[i]][1];\n        } else if (keys[i] === '-2') {\n          this.chartValue.correlation[2][0] = corrInd[indName[0]][keys[i]][0];\n          this.chartValue.correlation[2][1] = corrInd[indName[0]][keys[i]][1];\n        } else if (keys[i] === '-3') {\n          this.chartValue.correlation[3][0] = corrInd[indName[0]][keys[i]][0];\n          this.chartValue.correlation[3][1] = corrInd[indName[0]][keys[i]][1];\n        } else if (keys[i] === '-4') {\n          this.chartValue.correlation[4][0] = corrInd[indName[0]][keys[i]][0];\n          this.chartValue.correlation[4][1] = corrInd[indName[0]][keys[i]][1];\n        }\n      }\n    },\n    // 读冒号前的字符串\n    getStr() {\n      for (let i = 0; i < 5; i++) {\n        const str = ['', ''];\n        for (let j = 0; j < 2; j++) {\n          for (let k = 0; k < this.chartValue.correlation[i][j].split().length; k++) {\n            if (this.chartValue.correlation[i][j].split()[k] === ':') {\n              break;\n            }\n            str[j] += this.chartValue.correlation[i][j].split()[k];\n          }\n        }\n        this.chartValue.correlationName[i] = str;\n      }\n      console.log(this.chartValue.correlationName);\n    }\n  },\n  mounted() {\n    this.getBtnData();\n    this.getStr();\n  }\n};","map":{"version":3,"names":["view4","corrInd","name","components","data","chartValue","targetInd","correlation","idx","correlationName","methods","getBtnData","indName","Object","getOwnPropertyNames","keys","i","getStr","str","j","k","split","length","console","log","mounted"],"sources":["/Users/younggaming/Documents/webstorm/ybcprogram/src/views/page3.vue"],"sourcesContent":["<template>\n  <div class=\"container1\">\n    <div class=\"top\">\n      <button class=\"btn\">总资产</button>\n      <button class=\"btn\">总负债</button>\n      <button class=\"btn\">净利润</button>\n      <button class=\"btn\">营业总支出</button>\n      <button class=\"btn\">营业总收入</button>\n      <button class=\"btn\">经营现金流</button>\n      <button class=\"btn\">投资现金流</button>\n      <button class=\"btn\">融资现金流</button>\n    </div>\n    <div class=\"below\">\n      <button class=\"tarInd\">{{chartValue.targetInd}}</button>\n      <div class=\"corrBtn\" v-for=\"(p,index) in chartValue.correlation\" :key=\"p\">\n        <div class=\"marginDiv\">\n          <h4 class=\"text\">{{chartValue.idx[index]}}</h4>\n          <button class=\"corr\">{{p[0]}}</button><br>\n          <button class=\"corr\">{{p[1]}}</button>\n        </div>\n      </div>\n    </div>\n  </div>\n  <div class=\"container\">\n    <view4></view4>\n  </div>\n</template>\n\n<script>\nimport view4 from '@/components/view4.vue'\nimport corrInd from '../../../../pyCharm/ybcProject/data/corr_ind.json'\nexport default {\n  name: 'page_3',\n  components: {\n    view4\n  },\n  data () {\n    return {\n      chartValue: {\n        targetInd: '',\n        correlation: [['', ''], ['', ''], ['', ''], ['', ''], ['', '']],\n        idx: [0, -1, -2, -3, -4],\n        correlationName: [['', ''], ['', ''], ['', ''], ['', ''], ['', '']]\n      }\n    }\n  },\n  methods: {\n    // 按钮数据读取\n    getBtnData () {\n      const indName = Object.getOwnPropertyNames(corrInd)\n      this.chartValue.targetInd = indName[0]\n      const keys = Object.keys(corrInd[indName[0]])\n      for (let i = 0; i < 5; i++) {\n        if (keys[i] === '0') {\n          this.chartValue.correlation[0][0] = corrInd[indName[0]][keys[i]][0]\n          this.chartValue.correlation[0][1] = corrInd[indName[0]][keys[i]][1]\n        } else if (keys[i] === '-1') {\n          this.chartValue.correlation[1][0] = corrInd[indName[0]][keys[i]][0]\n          this.chartValue.correlation[1][1] = corrInd[indName[0]][keys[i]][1]\n        } else if (keys[i] === '-2') {\n          this.chartValue.correlation[2][0] = corrInd[indName[0]][keys[i]][0]\n          this.chartValue.correlation[2][1] = corrInd[indName[0]][keys[i]][1]\n        } else if (keys[i] === '-3') {\n          this.chartValue.correlation[3][0] = corrInd[indName[0]][keys[i]][0]\n          this.chartValue.correlation[3][1] = corrInd[indName[0]][keys[i]][1]\n        } else if (keys[i] === '-4') {\n          this.chartValue.correlation[4][0] = corrInd[indName[0]][keys[i]][0]\n          this.chartValue.correlation[4][1] = corrInd[indName[0]][keys[i]][1]\n        }\n      }\n    },\n    // 读冒号前的字符串\n    getStr () {\n      for (let i = 0; i < 5; i++) {\n        const str = ['', '']\n        for (let j = 0; j < 2; j++) {\n          for (let k = 0; k < this.chartValue.correlation[i][j].split().length; k++) {\n            if (this.chartValue.correlation[i][j].split()[k] === ':') {\n              break\n            }\n            str[j] += this.chartValue.correlation[i][j].split()[k]\n          }\n        }\n        this.chartValue.correlationName[i] = str\n      }\n      console.log(this.chartValue.correlationName)\n    }\n  },\n  mounted () {\n    this.getBtnData()\n    this.getStr()\n  }\n}\n</script>\n\n<style scoped>\n\n.container1 {\n  width: 1700px;\n  height: 150px;\n  /*border: 2px solid black;*/\n}\n\n.top {\n  width: 100%;\n  height: 20%;\n  display: flex;\n  /*border: 1px dashed black;*/\n}\n\n.btn {\n  width: 160px;\n  height: 100%;\n  background: #9DC3E6;\n  margin-left: 40px;\n  border-color: white;\n  font-size: 16px;\n}\n\n.below {\n  width: 100%;\n  height: 60%;\n  display: flex;\n  margin-top: 20px;\n}\n\n.tarInd {\n  width: 80px;\n  height: 60%;\n  margin-top: 35px;\n  background: #366AB9;\n  color: white;\n  font-size: 16px;\n}\n\n.corrBtn {\n  width: 15%;\n  height: 100%;\n  display: flex;\n  margin-left: 40px;\n  /*border: 1px dashed black;*/\n}\n\n.marginDiv {\n  margin-left: 1px;\n  width: 100%;\n}\n\n.text {\n  margin-top: 0;\n  margin-bottom: 0;\n}\n\n.corr {\n  margin-top: 10px;\n  width: 200px;\n  background: #54B435;\n  color: white;\n  border-color: white;\n}\n\n</style>\n"],"mappings":"AA6BA,OAAOA,KAAI,MAAO,wBAAuB;AACzC,OAAOC,OAAM,MAAO,mDAAkD;AACtE,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,UAAU,EAAE;QACVC,SAAS,EAAE,EAAE;QACbC,WAAW,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,CAAC;QAC/DC,GAAG,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;QACxBC,eAAe,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC;MACpE;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,UAASA,CAAA,EAAK;MACZ,MAAMC,OAAM,GAAIC,MAAM,CAACC,mBAAmB,CAACb,OAAO;MAClD,IAAI,CAACI,UAAU,CAACC,SAAQ,GAAIM,OAAO,CAAC,CAAC;MACrC,MAAMG,IAAG,GAAIF,MAAM,CAACE,IAAI,CAACd,OAAO,CAACW,OAAO,CAAC,CAAC,CAAC,CAAC;MAC5C,KAAK,IAAII,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,CAAC,EAAEA,CAAC,EAAE,EAAE;QAC1B,IAAID,IAAI,CAACC,CAAC,MAAM,GAAG,EAAE;UACnB,IAAI,CAACX,UAA
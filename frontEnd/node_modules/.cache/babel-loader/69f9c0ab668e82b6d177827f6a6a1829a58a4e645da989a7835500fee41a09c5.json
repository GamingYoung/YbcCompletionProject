{"ast":null,"code":"import view1 from '@/components/view11.vue';\nimport Table from '@/components/table.vue';\nimport axios from 'axios';\nimport corrCoppJson from '../../../../pyCharm/ybcProject/data/corr_copp.json';\nimport corrDetail from '../../../../pyCharm/ybcProject/data/corr_detail.json';\nexport default {\n  name: 'page_1',\n  data() {\n    return {\n      indicator: [['总资产', '固定资产', '流动资产'], ['总负债', '应付账款', '预收账款'], ['营业总支出', '营业支出', '各项费用'], ['营业总收入', '营业时间', '净利润'], ['经营现金流', '经营收入', '职工支出'], ['投资现金流', '投资收益', '长期投资'], ['融资现金流', '', '']],\n      copName: 'ST国华',\n      k: 5,\n      corCopName: ['海量数据', '普元信息', '国新健康', '照菱信息', '依米康', '纬德信息'],\n      // 左中子公司\n      corTableTitle: [{\n        name: '公司细节'\n      }, {\n        name: '目标公司'\n      }, {\n        name: '子公司'\n      }],\n      corTableValue: [{\n        公司细节: '公司代码',\n        目标公司: '000004.SZ',\n        子公司: '603138.SH'\n      }, {}]\n    };\n  },\n  components: {\n    view1,\n    Table\n  },\n  methods: {\n    // 搜索公司和K\n    search() {\n      const data = {\n        copName: this.copName,\n        k: this.k\n      };\n      axios.post('http://127.0.0.1:5000/search', data).then(res => {\n        const keys = Object.keys(corrCoppJson);\n        for (let i = 0; i < 3; i++) {\n          for (let j = 0; j < 2; j++) {\n            this.corCopName[i * 2 + j] = corrCoppJson[keys[i]][j];\n            this.corCopName.slice(i * 2 + j, 1, corrCoppJson[keys[i]][j]);\n          }\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    updateTable() {\n      return 0;\n    }\n  }\n};","map":{"version":3,"names":["view1","Table","axios","corrCoppJson","corrDetail","name","data","indicator","copName","k","corCopName","corTableTitle","corTableValue","公司细节","目标公司","子公司","components","methods","search","post","then","res","keys","Object","i","j","slice","catch","error","console","log","updateTable"],"sources":["/Users/younggaming/Documents/webstorm/ybcprogram/src/views/AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <!--左侧-->\n    <div class=\"panel\">\n      <!--左上-->\n      <div class=\"search\">\n        <input class=\"searchCop\" v-model.trim=\"copName\" placeholder=\"请输入公司名称\">\n        <input class=\"searchK\" v-model.trim=\"k\" placeholder=\"请输入类群个数\">\n        <button class=\"searchBtn\" @click=\"search\">搜索</button>\n      </div>\n      <!--左中-->\n      <div class=\"corCopInfo marginSub\">\n        <div class=\"corCopDiv\">资产负债：\n          <button class=\"corCopBtn\" @click=\"updateTable\">{{corCopName[0]}}</button>\n          <button class=\"corCopBtn\">{{corCopName[1]}}</button>\n        </div>\n        <div class=\"corCopDiv\">营业收入：\n          <button class=\"corCopBtn\">{{corCopName[2]}}</button>\n          <button class=\"corCopBtn\">{{corCopName[3]}}</button>\n        </div>\n        <div class=\"corCopDiv\">现金流量：\n          <button class=\"corCopBtn\">{{corCopName[4]}}</button>\n          <button class=\"corCopBtn\">{{corCopName[5]}}</button>\n        </div>\n      </div>\n      <!--左下-->\n      <div class=\"copInfo marginSub\">\n        <Table :fields=\"corTableTitle\" :rows=\"corTableValue\" :mulSelect=\"mulSelect\" />\n      </div>\n    </div>\n    <div class=\"view1 margin\">\n      <div class=\"btn\">\n        <div class=\"btnSub\" v-for=\"name in indicator\" :key=\"name\">\n          <button class=\"btnSubSub\" @click=\"this.getCorrCoppJson()\">{{name[0]}}</button>\n          <button class=\"btnSubSub\">{{name[1]}}</button>\n          <button class=\"btnSubSub\">{{name[2]}}</button>\n        </div>\n      </div>\n      <div class=\"view1_1 marginSub\">\n        <view1></view1>\n      </div>\n      <div class=\"view1_2 marginSub\"></div>\n    </div>\n    <div class=\"view2 margin\">\n    </div>\n  </div>\n</template>\n\n<script>\nimport view1 from '@/components/view11.vue'\nimport Table from '@/components/table.vue'\nimport axios from 'axios'\nimport corrCoppJson from '../../../../pyChar